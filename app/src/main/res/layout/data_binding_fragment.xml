<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>
        <import type="dnt.com.androidbestpractices.androidx.databinding.ui.databinding.DataBindingViewModel"/>
        <variable name="model" type="DataBindingViewModel"/>
    </data>

    <android.support.constraint.ConstraintLayout
        android:id="@+id/databinding"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".androidx.databinding.ui.databinding.DataBindingFragment">

        <TextView
            android:id="@+id/message"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="DataBindingFragment"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </android.support.constraint.ConstraintLayout>
</layout>